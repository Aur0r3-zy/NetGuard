# 数据库配置
DB_HOST=localhost
DB_PORT=3306
DB_NAME=network_detection
DB_USER=root
DB_PASSWORD=

# 日志配置
LOG_LEVEL=INFO
LOG_FILE=logs/app.log
LOG_MAX_SIZE=10485760
LOG_BACKUP_COUNT=5

# 算法配置
ALGORITHM_THRESHOLD=0.85
ALGORITHM_MEMORY_SIZE=1000
ALGORITHM_AFFINITY_THRESHOLD=0.7
ALGORITHM_CONCENTRATION_DECAY=0.1

# 网络配置
NETWORK_PACKET_BUFFER_SIZE=1000
NETWORK_PROTOCOLS=TCP,UDP,ICMP
NETWORK_COMMON_PORTS=80,443,22,21,23,25,53
NETWORK_SUSPICIOUS_PORTS=3389,445,1433,3306

# 预处理配置
PREPROCESSING_NORMALIZATION_METHOD=zscore
PREPROCESSING_FEATURES=packet_size,inter_arrival_time,protocol_entropy 